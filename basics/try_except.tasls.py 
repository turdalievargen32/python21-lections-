task7
try:
    age = int(input())

    if age<18:
        raise ValueError('Доступ запрещён')
except ValueError:
    print('Введён некорректный возраст')
else:
    print('Спасибо')
finally:
    print('До свидания!')


try:
    age = int(input())

    if age<18:
        print('Доступ запрещён')
except:
    print('Введён некорректный возраст')
else:
    print('Спасибо')
finally:
    print('До свидания!')

